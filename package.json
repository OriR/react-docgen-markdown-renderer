{
  "name": "react-docgen-markdown-renderer",
  "version": "2.1.3",
  "description": "Renders a markdown template based on a react-docgen object",
  "main": "lib/index.js",
  "scripts": {
    "test": "lab",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/.bin/lab --coverage-exclude lib",
    "test:coverage:html": "lab -r console -o stdout -r html -o coverage.html"
  },
  "keywords": [
    "react",
    "react-docgen",
    "component",
    "md",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OriR/react-docgen-markdown-renderer"
  },
  "author": "Ori Riner",
  "license": "MIT",
  "peerDependencies": {
    "react-docgen": "^2 || ^3"
  },
  "dependencies": {
    "react-docgen-renderer-template": "^0.1.0"
  },
  "devDependencies": {
    "code": "^5.2.0",
    "husky": "^1.3.1",
    "lab": "^15.5.0",
    "lint-staged": "^13.1.0",
    "prettier": "^1.16.4",
    "react-docgen": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
